# ntfy topic for receiving commands (use a long random string for privacy)
COMMANDS_TOPIC=hello-cmd-REPLACE-WITH-UUID

# ntfy topic for publishing results
RESULTS_TOPIC=hello-res-REPLACE-WITH-UUID

# ntfy server URL
NTFY_BASE_URL=https://ntfy.sh

# Optional ntfy auth token (leave empty if not using token auth)
NTFY_TOKEN=

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
